Print
    screenshot

super + l
    setxkbmap -layout us; statusbar; lock

super + @space
    changelayout; statusbar

XF86AudioMute
    pamixer -t; statusbar
XF86AudioRaiseVolume
    pamixer -i 5; dunstify -t 700 -r "7296431" "Volume" `pamixer --get-volume-human`; statusbar
XF86AudioLowerVolume
    pamixer -d 5; dunstify -t 700 -r "7296431" "Volume" `pamixer --get-volume-human`; statusbar

